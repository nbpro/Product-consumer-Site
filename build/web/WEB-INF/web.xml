<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginvalidate</servlet-name>
        <servlet-class>loginvalidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newregister</servlet-name>
        <servlet-class>newregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_login_validate</servlet-name>
        <servlet-class>admin_login_validate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>available</servlet-name>
        <servlet-class>available</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>putorder</servlet-name>
        <servlet-class>putorder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>placeorder</servlet-name>
        <servlet-class>placeorder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processorder</servlet-name>
        <servlet-class>processorder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginvalidate</servlet-name>
        <url-pattern>/loginvalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newregister</servlet-name>
        <url-pattern>/newregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_login_validate</servlet-name>
        <url-pattern>/admin_login_validate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>available</servlet-name>
        <url-pattern>/available</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>putorder</servlet-name>
        <url-pattern>/putorder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>placeorder</servlet-name>
        <url-pattern>/placeorder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processorder</servlet-name>
        <url-pattern>/processorder</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
